<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id, list: "genres" %>
  <datalist id="genres">
    <option value="H">
  </datalist>
  <br />

  <%= f.submit %>
<% end %>
